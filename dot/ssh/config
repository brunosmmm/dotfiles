Host localhost
     Port 2222
     StrictHostKeyChecking no
     UserKnownHostsFile = /dev/null

Host gateway.coe.neu.edu
	ControlMaster auto
	ControlPath ~/.ssh/%r@%h:%p
	ServerAliveCountMax=30
	ServerAliveInterval=5

Host diabase.ece.neu.edu
  ControlMaster auto
  ControlPath ~/.ssh/master-%r@%h:%p